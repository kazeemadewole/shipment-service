{
  "name": "shipment-service",
  "version": "1.2.0",
  "description": "Tax tech shipment service",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "npm run prepare:start",
    "start:jobs": "npm run prepare:jobs",
    "dev": "npm run build && ts-node-dev ./src/index.ts",
    "dev:env": "npm run build && ts-node-dev ./src/index.ts",
    "dev:jobs": "npm run build && ts-node-dev ./src/jobs.ts",
    "prepare:start": "node ./dist/index.js",
    "prepare:jobs": "node ./dist/jobs.js",
    "build": "rimraf dist && npx tsc -b",
    "lint": "eslint src/**/*.ts",
    "lint-staged": "lint-staged",
    "format": "eslint src/**/*.ts --fix",
    "test": "APP_ENV=test jest --forceExit --verbose",
    "test-debug": "APP_ENV=test node --inspect-brk=0.0.0.0:5859 ./node_modules/jest/bin/jest.js --runInBand --forceExit --verbose",
    "test:coverage": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "prepare": "if [ \"$CI\" != \"true\" ]; then husky; fi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
